CREATE TABLE user (
  user_id CHAR(8) PRIMARY KEY,
  user_name VARCHAR(10) NOT NULL,
  mail VARCHAR(255) NOT NULL UNIQUE,
  password VARCHAR(255) NOT NULL,
  profile_image BLOB,
  bio VARCHAR(255)
);



